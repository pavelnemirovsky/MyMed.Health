  },
  "dependencies": {
    "mermaid": "^11.12.1",
    "next": "^15.5.5",
    "next-auth": "^5.0.0-beta.30",
    "next-intl": "^4.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.2",
    "eslint-plugin-next-on-pages": "^1.13.8",
    "typescript": "^5"
  }
}
