{
  "name": "mymed.health",
  "version": "0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "build:cf": "next build && npm run pages:build",
    "start": "next start",
    "lint": "next lint",
    "pages:build": "GENERATE_SOURCEMAP=false npx @cloudflare/next-on-pages",
